{
  "name": ".lazyDev",
  "description": "Easy copy and paste for social network fields and others",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "declarativeContent"],

  "action": {
    "default_popup": "../ext/popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "64": "images/icon64.png"
    }
  },

    "commands": {
      "_execute_action": {
        "suggested_key": {
          "default": "Alt+C"
        }
      }
    },

    "icons": {
      "16": "images/icon16.png",
      "64": "images/icon64.png"
    }
}
